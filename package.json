{
  "name": "css-frameworks-ca",
  "version": "1.0.5",
  "description": "Test",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "lint-fix": "eslint \"**/*.js\" --cache --fix",
    "prepare": "husky install",
    "test": "npm run test-unit",
    "test-unit": "jest"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "eslint-plugin-jest": "^28.8.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
