{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "cypress/globals": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:cypress/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
      "cypress/no-unnecessary-waiting": "off"
    }
  }
  